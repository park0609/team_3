    "a = input(\"문장을 입력해주세요!\")\n",
    "e = a.replace(\".\",\"\")\n",
    "b = e.split()\n",
    "c = []\n",
    "for n in range(b.index(b[-1])+1):\n",
    "    d = len(b[n])\n",
    "    c.append(d)\n",
    "    f = c.index(max(c))\n",
    "    g = b[f]\n",
    "print(g)"
