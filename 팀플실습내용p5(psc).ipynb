   "source": [
    "text = input()\n",
    "a = text.replace(\".\",\"\")\n",
    "b = a.split()\n",
    "d = []\n",
    "h = []\n",
    "for n in range(0,len(b)):\n",
    "    f = b[n]\n",
    "    c = b[n][0:1]\n",
    "    d = c.upper()\n",
    "    e = f.replace(str(b[n][0]),d[0])\n",
    "    h.append(e)\n",
    "    g = \" \".join(h)\n",
    "print(g)"
   ]
